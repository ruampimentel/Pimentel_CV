@article{brandao_impacto_2017,
  title = {Impacto de Exergames na qualidade de vida de idosos},
  author = {Brandão, Pierre Soares and Neto, Ivo Vieira de Sousa and Rauber, Suliane Beatriz and Pimente, Ruam Pedro Francisco de Assis and Zukowski, Michelle Sales and Fagundes, Fabiano and Campbell, Carmen Silvia Grubert},
  date = {2017-06-25},
  journaltitle = {Fisioterapia Brasil},
  volume = {18},
  number = {3},
  pages = {320--328},
  issn = {2526-9747},
  doi = {10.33233/fb.v18i3.1058},
  url = {https://portalatlanticaeditora.com.br/index.php/fisioterapiabrasil/article/view/1058},
  urldate = {2022-01-10},
  abstract = {Introdução: A Qualidade de Vida (QV) está relacionada a interações de aspectos fí­sico, psicológico, social e ambiental do ser humano. Com o aumento da expectativa de vida em todo o mundo, surgiu a necessidade de se desenvolver estratégias para manutenção da QV durante o envelhecimento. Os exergames têm sido considerados uma possibilidade de intervenção lúdica para manutenção e melhora da QV em todas as idades. Objetivo: Avaliar a influência de exergames que utilizam o Kinect sobre a QV de idosos sedentários. Métodos: Seis idosos (n = 2M; n = 4F; 64,83 ± 4,07 anos) participaram de 2 sessões semanais de 30 min de atividade de jogos com Kinect durante seis meses. A versão curta do questionário World Health Organization Quality of Life (WHOQOL) foi aplicada pré e pós-intervenção. Resultados: Tanto a QV Geral quanto o Domí­nio Ambiental melhoraram significativamente após a intervenção (67,72 ± 10,95 vs. 70,82 ± 9,10, p = 0,01; 55,38 ± 3,50 vs. 64,06 ± 6,47, p = 0,02) respectivamente. Já os domí­nios Fí­sico e Psicológico não apresentaram alteração significativa após a intervenção (67,26 ± 19,35 vs. 72,02 ± 15,87, p = 0,45; 70,13 ± 22,42 vs. 73,61 ± 7,75, p = 0,69) respectivamente. Conclusão: A intervenção com jogos de videogame ativo com Kinect interferiu positivamente na QV geral e no domí­nio ambiental dos idosos participantes do estudo.Palavras-chave: qualidade de vida, idoso, exergames.},
  issue = {3},
  langid = {portuguese},
  file = {/Users/ruampimentel/Library/CloudStorage/OneDrive-UniversityofToledo/Books and Papers/Brandão et al - 2017 - Impacto de Exergames na qualidade de vida de idosos.pdf}
}

@article{goncalves_funcionamento_2020,
  title = {Funcionamento patológico da personalidade para predição da depressão, qualidade de vida e esperança},
  author = {Gonçalves, André Pereira and Pimentel, Ruam Pedro Francisco de Assis and Carvalho, Lucas De Francisco},
  date = {2020-04-30},
  journaltitle = {Interação em Psicologia},
  shortjournal = {Inter. Psicol.},
  volume = {24},
  number = {1},
  issn = {1981-8076},
  doi = {10.5380/psi.v24i1.62784},
  url = {https://revistas.ufpr.br/psicologia/article/view/62784},
  urldate = {2020-08-22},
  abstract = {Estudos prévios apontam que níveis mais severos da personalidade (isto é, funcionamentos patológicos da personalidade – FPP) se associam com maior presença de sintomas depressivos, baixa qualidade de vida e esperança. Entretanto, não foram observados estudos utilizando conjuntamente essas três variáveis em um modelo focado no FPP. O objetivo deste trabalho foi verificar a capacidade preditiva dos níveis de FPP sobre qualidade de vida (QV), esperança e depressão. Participaram 475 sujeitos que responderam: Inventário Dimensional Clínico versão triagem (IDCP-triagem), Escala Baptista de depressão versão triagem (EBADEP-A-triagem), WHOQOL-Bref e Escala de Esperança de Herth (EEH). Por meio da modelagem de equações estruturais, indicadores de depressão foram positivamente preditos pelo FPP, enquanto os de QV e esperança foram negativamente preditos, tal qual as hipóteses do estudo. Estes achados demonstram a alta capacidade preditiva do FPP sobre depressão, QV e esperança. Na clínica, ao observar perfil que combine estas características pode ser indicado a necessidade da avaliação dos aspectos patológicos da personalidade.},
  langid = {portuguese},
  file = {/Users/ruampimentel/Library/CloudStorage/OneDrive-UniversityofToledo/Books and Papers/Gonçalves et al_2020_Funcionamento patológico da personalidade para predição da depressão, qualidade de vida e esperança.pdf}
}

